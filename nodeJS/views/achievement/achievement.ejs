<!DOCTYPE html>
<html lang="en">
<head>
  <title>achievement</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"></script>


  <link href="stylesheets/achievement/style.css" rel="stylesheet" type="text/css">
</head>
<body>

  <%- include ../layer/header %>
  <br></br>
  <div class="container">
    <div class="row">

      <div class="col-xs-12 col-sm-7">
        <center><h3>Achievement</h3></center>
        <div class="box">
        
          <div class="badge orange">
            <div class="circle"> <i class="fa fa-wheelchair-alt"></i></div>
            <div class="ribbon">BB</div>
          </div>
          <div class="badge pink">
            <div class="circle"> <i class="fa fa-pied-piper"></i></div>
            <div class="ribbon">CC</div>
          </div>
          <div class="badge red">
            <div class="circle"> <i class="fa fa-shield"></i></div>
            <div class="ribbon">EE</div>
          </div>
        
        </div>

        <div class="col-xs-12 col-sm-12">
          <div class="box">
  
            <div class="badge purple">
              <div class="circle"> <i class="fa fa-anchor"></i></div>
              <div class="ribbon">FF</div>
            </div>
            <div class="badge teal">
              <div class="circle"> <i class="fa fa-bicycle"></i></div>
              <div class="ribbon">GG</div>
            </div>
            <div class="badge blue">
              <div class="circle"> <i class="fa fa-users"></i></div>
              <div class="ribbon">HH</div>
            </div>

          </div>
        </div>

        <div class="col-xs-12 col-sm-12">
          <div class="box">

            <div class="badge blue-dark">
              <div class="circle"> <i class="fa fa-rocket"></i></div>
              <div class="ribbon">II</div>
            </div>
            <div class="badge green">
              <div class="circle"> <i class="fa fa-tree"></i></div>
              <div class="ribbon">JJ</div>
            </div>
            <div class="badge green-dark">
              <div class="circle"> <i class="fa fa-user fa-street-view"></i></div>
              <div class="ribbon">KK</div>
            </div>
          </div>
        </div>     

        <div class="col-xs-12 col-sm-12">
          <div class="box">
            
            <div class="badge silver">
              <div class="circle"> <span class="font"></span></div>
              <div class="ribbon">LL</div>
            </div>
            <div class="badge gold">
              <div class="circle"> <i class="fa fa-magic"></i></div>
              <div class="ribbon">NN</div>
            </div>
          </div>
        </div>     
  
      </div>
      
      <center><h3>Leaderboard</h3></center>
      <%- include leader %>

         
        </div>
      </div>

    
      

    </div>
  </div>
</body>
</html>